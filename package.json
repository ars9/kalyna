{
  "name": "@li0ard/kalyna",
  "version": "0.1.4",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "author": "li0ard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/li0ard/kalyna.git"
  },
  "bugs": {
    "url": "https://github.com/li0ard/kalyna/issues"
  },
  "homepage": "https://github.com/li0ard/kalyna#readme",
  "description": "Kalyna (DSTU 7624:2014) cipher implementation in pure TypeScript",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist/ && npm run build:esm && npm run build:cjs && npm run build:cjs-package",
    "build:esm": "tsc --project tsconfig.build.json",
    "build:cjs": "tsc --project tsconfig.build.cjs.json",
    "build:cjs-package": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "docs": "typedoc --options .config/typedoc/config.cjs"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typedoc": "^0.28.9"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@li0ard/gost3413": "^0.1.6"
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["dstu", "crypto", "kalyna", "dstu7624"]
}